@using OnlineShopPodaci.Model;
@{

    Layout = "~/Views/Shared/_Layout.cshtml";


}
@model List<OnlineShop.ViewModels.LookInCartVM>
    <style>
        #container{
            margin:30px;
        }
        #tabela{
            background-color: #d9d9d9;
            padding:20px;
            width:90%;
            margin:auto;
            border-radius:10px;
        }
    </style>
<body>
    <div id="container">
        <h2 style="color:maroon;margin-left:60px;">Košarica</h2> <br />

        <div id="tabela">

        </div>

    </div>
</body>

<script>

        $.get("/Cart/GetCartItems", function (res) {
            $("#tabela").html(res);
        });

</script>